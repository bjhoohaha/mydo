<%= render "header" %>
<br>
</br>
<div id = "time-alert">
  <% unless @task_today.empty? %>
    <div id = "today">
      <h1>Today</h1>
      <br>
      </br>
        <%= render "table", task_parameter: @task_today, del:false %>
      <br>
      </br>
    </div>
  <% end %>
  <% unless @task_week.empty? %>
    <div id = "week">
      <h1>This Week</h1>
      <br>
      </br>
        <%= render "table", task_parameter: @task_week, del:false %>
        <br>
      </br>
    </div>
  <% end %>
</div>
